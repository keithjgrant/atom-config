"*":
  "atom-css-comb":
    commonConfigs: "/Users/kgrant/src/web/icegroupweb-styles/.csscomb.json"
  "atom-material-ui":
    colors:
      abaseColor: "#193d65"
      accentColor: "#fbc459"
    tabs:
      compactTabs: true
      tintedTabBar: true
    ui:
      panelContrast: true
  core:
    closeEmptyWindows: false
    destroyEmptyPanes: false
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "shindig-syntax"
    ]
    titleBar: "custom"
  editor:
    fontFamily: "FiraMono-Regular"
    fontSize: 13
    invisibles:
      eol: " "
    preferredLineLength: 90
    scrollPastEnd: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "a415bc28-908e-1e13-cbbc-213f597e9145"
  "git-diff": {}
  hyperclick:
    darwinTriggerKeys: "altKey,metaKey"
  linter: {}
  "linter-eslint": {}
  "linter-stylelint":
    disableWhenNoConfig: false
    useStandard: true
  "linter-ui-default":
    panelHeight: 66
    showPanel: true
  minimap:
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "one-dark-ui":
    hideDockButtons: true
  perfectionist:
    cascade: false
    formatOnSave: true
    indentSize: 2
    trimLeadingZero: false
  "prettier-atom":
    bracketSpacing: false
    formatOnSave: false
    formatOnSaveOptions:
      cssScopes: [
        "source.css"
        "source.css.postcss"
        "source.css.less"
        "source.css.scss"
      ]
      enabled: true
    prettierEslintOptions: {}
    prettierOptions:
      bracketSpacing: false
      printWidth: 90
      singleQuote: true
      trailingComma: "es5"
    printWidth: 90
    silenceErrors: false
  stylefmt: {}
  tabs: {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  "vim-mode-plus":
    keymapPToPutWithAutoIndent: true
    notifiedCoffeeScriptNoLongerSupportedToExtendVMP: true
    startInInsertMode: true
  welcome:
    showOnStartup: false
